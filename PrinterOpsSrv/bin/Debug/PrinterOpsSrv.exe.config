<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>

  </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <log4net >
    <appender name="EventLogAppender" type="log4net.Appender.EventLogAppender" >
      <applicationName value="TestNcron" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger [%property{NDC}] - %message%newline" />
      </layout>
    </appender>

    <appender name="FileAppender" type="log4net.Appender.FileAppender">
      <file value="log-file.txt" />
      <appendToFile value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger [%property{NDC}] - %message%newline" />
      </layout>
    </appender>

    <root>
      <level value="ERROR" />
      <appender-ref ref="EventLogAppender" />
      <appender-ref ref="FileAppender" />
    </root>

  </log4net>
  <appSettings>
    <add key="cron" value="*/5 * * * *"/>
    <add key="listAddresses" value="C:\SOFTZ\TEST\TestSNMP\listIPAddresses.txt"/>
  </appSettings>
  <system.serviceModel>
    <bindings>
      <netTcpBinding>
        <binding name="NetTCPBinding_IPOService">
          <security mode="None" />
        </binding>
      </netTcpBinding>
    </bindings>
    <client>
      <endpoint address="net.tcp://10.136.64.1/PrinterOpsService/POService.svc"
        binding="netTcpBinding" bindingConfiguration="NetTCPBinding_IPOService"
        contract="POServiceRef.IPOService" name="NetTCPBinding_IPOService" />
    </client>
  </system.serviceModel>
</configuration>